    "debug": "^4.4.1",
    "es-toolkit": "^1.39.8",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.1",
    "git-config": "^0.0.7",
    "globby": "^14.1.0",
    "got": "14",
    "json5": "^2.2.3",
    "mem-fs": "^4.1.2",
    "moment": "^2.30.1",
    "p-props": "^6.0.0",
    "promise.ify": "^0.4.0",
    "swig-templates": "^2.0.3",
    "umi-request": "^1.4.0",
    "yeoman-generator": "^7.5.1"
  },
  "devDependencies": {
    "@magicdawn/eslint-config": "^1.5.9",
    "@magicdawn/prettier-config": "^0.1.0",
