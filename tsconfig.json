{
  "include": ["src/**/*"],
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "generators",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "target": "es2019",
    "module": "CommonJS",
    "strict": false,
    "skipLibCheck": true,
    "incremental": true,
    "tsBuildInfoFile": "generators/tsconfig.tsbuildinfo"
  }
}
