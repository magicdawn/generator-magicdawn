    "@types/swig": "^0.0.34",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.4",
    "prettier": "^3.6.2",
    "type-fest": "^4.41.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "yeoman-environment": "^4.4.3"
  },
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "unrs-resolver"
    ]
  },
  "lint-staged": {
    "*.{?(c|m)(j|t)s?(x),json,y?(a)ml}": [
      "eslint --fix",
      "prettier --write"
