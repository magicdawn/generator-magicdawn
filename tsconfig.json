{
  "include": ["src/**/*"],
  "compilerOptions": {
    "tsBuildInfoFile": "generators/tsconfig.tsbuildinfo",
    "rootDir": "src",
    "outDir": "generators",
    "target": "ES2024",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noImplicitOverride": true,
    "skipLibCheck": true,
    "types": ["node", "@total-typescript/ts-reset"]
  }
}
